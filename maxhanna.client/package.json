{
  "name": "maxhanna.client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-script-os",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prestart": "node aspnetcore-https",
    "start:redirect": "node redirect-server.js",
    "start:windows": "start cmd /k \"npm run start:redirect\" & start cmd /k \"npm run start:https\" & start cmd /k \"npm run start:test\"",
    "start:default": "npm run start:redirect & npm run start:https & npm run start:test",
    "start:test": "ng serve --port 8000 --configuration test",
    "start:http": "ng serve --port 80 --configuration http",
    "start:https": "ng serve --port 443 --ssl true --ssl-cert ssl/bughosted_com.crt --ssl-key ssl/bughosted_com.key --configuration production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.4",
    "@angular/common": "^19.2.4",
    "@angular/compiler": "^19.2.4",
    "@angular/core": "^19.2.4",
    "@angular/forms": "^19.2.4",
    "@angular/platform-browser": "^19.2.4",
    "@angular/platform-browser-dynamic": "^19.2.4",
    "@angular/router": "^19.2.4",
    "chart.js": "^4.4.3",
    "firebase": "^12.4.0",
    "jest-editor-support": "*",
    "mupen64plus-web": "^2.0.0",
    "ng2-charts": "^6.0.1",
    "nostalgist": "^0.16.0",
    "run-script-os": "*",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.5",
    "@angular/cli": "^19.2.5",
    "@angular/compiler-cli": "^19.2.4",
    "@types/jasmine": "~5.1.0",
    "browserslist": "^4.24.4",
    "caniuse-lite": "^1.0.30001713",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
