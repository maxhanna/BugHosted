{
  "name": "maxhanna.client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-script-os",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prestart": "node aspnetcore-https",
    "start:redirect": "node redirect-server.js",
    "start:windows": "start cmd /k \"npm run start:redirect\" & start cmd /k \"npm run start:https\" & start cmd /k \"npm run start:test\"",
    "start:default": "npm run start:redirect & npm run start:https & npm run start:test",
    "start:test": "ng serve --port 8000 --configuration test --disable-host-check --memory 2g",
    "start:http": "ng serve --port 80 --configuration http --disable-host-check --memory 2g",
    "start:https": "node prod-server.js",
    "start:https:pm2": "pm2 start prod-server.js --name maxhanna-prod --instances 1 --max-memory-restart 1G --env NODE_ENV=production",
    "start:http:prod": "USE_HTTPS=false node prod-server.js",
    "stop:prod": "pm2 stop maxhanna-prod",
    "restart:prod": "pm2 restart maxhanna-prod",
    "logs:prod": "pm2 logs maxhanna-prod",
    "start:express": "node dev-server.js",
    "start:express:test": "DEV_PORT=8000 USE_HTTPS=false node dev-server.js",
    "start:express:https": "DEV_PORT=443 USE_HTTPS=true node dev-server.js",
    "start:pm2": "pm2 start dev-server.js --name maxhanna-dev --max-memory-restart 512M --instances 1 --watch src --ignore node_modules",
    "stop:pm2": "pm2 stop maxhanna-dev",
    "restart:pm2": "pm2 restart maxhanna-dev",
    "logs:pm2": "pm2 logs maxhanna-dev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.4",
    "@angular/common": "^19.2.4",
    "@angular/compiler": "^19.2.4",
    "@angular/core": "^19.2.4",
    "@angular/forms": "^19.2.4",
    "@angular/platform-browser": "^19.2.4",
    "@angular/platform-browser-dynamic": "^19.2.4",
    "@angular/router": "^19.2.4",
    "chart.js": "^4.4.3",
    "firebase": "^12.4.0",
    "jest-editor-support": "*",
    "mupen64plus-web": "^2.0.0",
    "ng2-charts": "^6.0.1",
    "nostalgist": "^0.16.0",
    "run-script-os": "*",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.5",
    "@angular/cli": "^19.2.5",
    "@angular/compiler-cli": "^19.2.4",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.10.0",
    "@types/youtube": "^0.1.2",
    "browserslist": "^4.24.4",
    "caniuse-lite": "^1.0.30001713",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "morgan": "^1.10.0",
    "pm2": "^5.3.0",
    "typescript": "~5.8.2"
  }
}
